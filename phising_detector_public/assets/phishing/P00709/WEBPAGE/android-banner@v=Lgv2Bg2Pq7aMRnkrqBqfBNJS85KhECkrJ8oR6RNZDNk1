$(document).ready(function(){var n=$("#smartbanner"),t;navigator.userAgent.match(/Android/i)&&n.html('<div class="sb-container"><a href="#" class="sb-close">&times;<\/a><div class="sb-info">Serve for Android<span>Available in the Play Store<\/span><\/div><a href="market://details?id=com.serve.mobile" class="sb-button"><span>View<\/span><\/a><\/div>'),t=n.find(".sb-close");t.on("click",function(){n.slideUp(200)})})