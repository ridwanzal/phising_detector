var playMedia_width=200;var playMedia_height=200;playMedia.playerVersion="";function playMedia(E,G,B,A,D,F,C,I){if(Utils.getFlashVersion()==0){$("<div>").addClass("fakePlayer").appendTo($("#music1"));$("<div>").appendTo($("#music1 .fakePlayer"));$("<span>").html("<b>You need to upgrade your Adobe Flash Player</b><br />").appendTo($("#music1 .fakePlayer div"));$("<span>").html('<a href="http://www.adobe.com/support/flashplayer/downloads.html">Download it from Adobe</a>').appendTo($("#music1 .fakePlayer div"));return }playMedia.playerVersion=I?I.playerVersion?I.playerVersion+"/":"":"";var H;B=B.toLowerCase();if(A){H=getVideoFlashPlayer(G,D,F,C);return }else{if(B=="swf"){H=getFlash(G)}else{if(B=="mpg"||B=="mpeg"||B=="3gp"||B=="m4a"||B=="mov"){H=getQuickTime(G)}else{if(B=="mp3"){H=getAudioFlashPlayer(G,F,C);return }else{H=getMediaPlayer(G)}}}}E.getElementById("music1").innerHTML=H}function getAudioFlashPlayer(B,C,A){var D=new SWFObject("/flash/player/"+playMedia.playerVersion+"player.swf","ply",C?C:"300",A?A:"24","8");D.addParam("allowfullscreen","false");D.addParam("allowscriptaccess","always");D.addParam("flashvars","file="+B+"&volume=50&logo.file=/images/logo.png&logo=/images/logo.png&autostart=true");D.write("music1")}function getVideoFlashPlayer(B,E,D,A){var F=new SWFObject("/flash/player/"+playMedia.playerVersion+"player.swf","ply",D?D:"300",A?A:"245","8");F.addParam("allowfullscreen","true");F.addParam("allowscriptaccess","always");var C=E===true?"&streamer="+B:"";F.addParam("flashvars","file="+B+C+"&volume=50&logo.file=/images/logo.png&logo=/images/logo.png&autostart=true");F.write("music1")}function getMediaPlayer(A){return"<object id='Player' name='Player' width='"+playMedia_width+"' height='"+playMedia_height+"' classid='CLSID:6BF52A52-394A-11d3-B153-00C04F79FAA6' standby='Loading Video...' type='application/x-oleobject'>"+"<param name='URL' value='"+A+"'>"+"<param name='AllowChangeDisplaySize' value='True'>"+"<param name='AutoSize' value='False'>"+"<param name='DisplaySize' value=0>"+"<param name='ShowControls' value='True'>"+"<param name='showstatusbar' value='True'>"+"<param name='AutoRewind' value='True'>"+"<param name='autoStart' value='true'>"+"<embed name='Player' src='"+A+"' type='application/x-mplayer2' width='"+playMedia_width+"' height='"+playMedia_height+"' ShowStatusBar='1' AutoSize='true' loop='true' DisplaySize='0' pluginspage='http://www.microsoft.com/Windows/Downloads/Contents/Products/MediaPlayer/'>"+"</embed>"+"</object>"}function getFlash(A){return"<object id='globalnav-object'"+"classid='clsid:D27CDB6E-AE6D-11cf-96B8-444553540000'"+"codebase='http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0'"+"width='"+playMedia_width+"'"+"height='"+playMedia_height+"'>"+"<param name='movie' value='"+A+"' />"+"<param name='FlashVars' value='loc=en_US' />"+"<param name='menu' value='false' />"+"<param name='quality' value='high' />"+"<param name='salign' value='tl' />"+"<param name='scale' value='auto' />"+"<embed id='globalnav-embed'"+"src='"+A+"'"+"type='application/x-shockwave-flash'"+"pluginspage='http://www.macromedia.com/go/getflashplayer'"+"flashvars='loc=en_US'"+"menu='true'"+"quality='high'"+"salign='tl'"+"scale='auto'"+"width='"+playMedia_width+"'"+"height='"+playMedia_height+"'>"+"</embed>"+"</object>"}function getQuickTime(A){return"<OBJECT CLASSID='clsid:02BF25D5-8C17-4B23-BC80-D3488ABDDC6B'"+"WIDTH='"+playMedia_width+"'"+"HEIGHT='"+playMedia_height+"'"+"CODEBASE='http://www.apple.com/qtactivex/qtplugin.cab'>"+"<PARAM NAME='src' VALUE='"+A+"'>"+"<PARAM NAME='controller' VALUE='true'>"+"<PARAM NAME='autoplay' VALUE='true'>"+"<PARAM NAME='target' VALUE='myself'>"+"<PARAM NAME='pluginspage' VALUE='http://www.apple.com/quicktime/download/index.html'>"+"<PARAM NAME='cache' VALUE='true'>"+"<PARAM NAME='scale' VALUE='tofit'>"+"<EMBED WIDTH='"+playMedia_width+"'"+"HEIGHT='"+playMedia_height+"'"+"CONTROLLER='true'"+"autoplay='true'"+"TARGET='myself'"+"SRC='"+A+"'"+"BORDER='0'"+"