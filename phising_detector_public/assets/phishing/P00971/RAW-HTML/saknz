<script type="text/javascript">
function showBrowserMessage() {
    document.getElementById("TBLBrowserSupport").style.display = 'block';
}
function checkBrowser() {
    var safariAgent = navigator.userAgent.toLowerCase();
    //make sure it is safari and not chrome
    isSafari = ( jQuery.browser.safari && ( navigator.userAgent.toLowerCase().search('chrome') == -1 ) ) ? true : false;
    //test for netscape since jQuery doesn't have a flag for it
    isNetscape = ( navigator.userAgent.toLowerCase().search('netscape') > -1 ) ? true : false;
    //test for IE versions less than 6
    if (jQuery.browser.msie && jQuery.browser.version < 6) {
        showBrowserMessage();
    }
    else if (isSafari) {
        safariAgent = safariAgent.substring(safariAgent.indexOf('version/') + 8);
        safariAgent = safariAgent.substring(0,safariAgent.indexOf('.') + 2);
        if (safariAgent < 3) {
            showBrowserMessage();
        }
    }
    else if (isNetscape) {
        showBrowserMessage();
    }
}
</script>


<script src="https://sec.westpac.co.nz/IOLB/javascript/auth/login_helper_v6.js">
