<script type="text/javascript">
var firstPacketTime=(new Date()).getTime();
function setCartCookie(A){
	document.cookie="CARTStart="+escape(A)+"; path=/";
	document.cookie="CARTPath="+getPagePath()+"; path=/";}
function startCart(){
	setCartCookie((new Date()).getTime());}
function getPagePath(){
	return escape(window.location.pathname);}
window.onbeforeunload=startCart;
</script>

