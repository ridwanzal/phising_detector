<script type="text/javascript">
var pageLandTime = new Date();
var loggingIn = false;
var sessionTimeOutSeconds = 600 * 1000;
var timeoutThreshold = 20000;
setTimeout(timeout, sessionTimeOutSeconds);

function onloadFunctions()
{
  checkBrowser();
  if (!testAcceptCookie())
  {
    document.location.href="#";
    return;
  }

  var pageHeaderContent="Login";
  var userIdPrompt="Enter your Customer ID";
  var passwordPrompt="Enter your password";

  var url = readCookie("WASReqURL");

  var foundReloginPhoneParameter = false;

  if (url != null)
  {
    var rlParam = getParameter(url, "relogin");

    if (rlParam != null && rlParam == "phone")
    {
      foundReloginPhoneParameter = true;
      pageHeaderContent="First time login for Phone Banking customers";
      userIdPrompt="Enter your Phone Banking ID -";
      passwordPrompt="Enter your Phone Banking password -";
    }

    if (rlParam != null && rlParam == "true")
    {
        var timedOutEle = document.getElementById("timedOut");
        timedOutEle.style.display="inline";
    }
  }

  var protocol = location.protocol;
  var host = location.host;
  var path = location.pathname;
  var webapp = path.substr(0, path.indexOf('/', 1));

  var redirectURL = protocol + "//" + host + webapp + "/newSession";

  if (foundReloginPhoneParameter)
  {
    redirectURL = redirectURL + "?relogin=phone";
  }

  createCookie("WASReqURL", redirectURL);

  var pageHeaderEle=document.getElementById("pageHeaderElement");
  var userIdPromptEle=document.getElementById("userIdPrompt");
  var passwordPromptEle=document.getElementById("passwordPrompt");
  pageHeaderEle.innerHTML=pageHeaderContent;
  userIdPromptEle.innerHTML=userIdPrompt;
  passwordPromptEle.innerHTML=passwordPrompt;


  createCookie("LoginCount", 0);


  var theForm=document.logonForm;
  theForm.style.visibility="visible";
  theForm.input_username.focus();

  document.getElementById("verisignFrame").src="https://sec.westpac.co.nz/IOLB/Verisign.jsp";

    jQuery(document).ready(function($) {
        deleteCookie("keystrokeBiometric");
        deleteCookie("pressedBackspace");
        deleteCookie("clickedMouse");
        var password = $("input[name=input_password]");
        password.initKeystrokeBiometric()
                .data("clicked", false)
                .mousedown(function() { password.data("clicked", true); });

        jQuery("#username").keydown(function() {
            testKeepAlive();
        });
        jQuery("#password").keydown(function() {
            testKeepAlive();
        });
    });
}
</script>

