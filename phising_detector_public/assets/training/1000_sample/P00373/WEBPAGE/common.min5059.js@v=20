function displayFlash(a,b){b=b in{error:1,success:1,warning:1,info:1}?b:"info";$("#contenuEA").before('<div class="alert alert-'+b+'"><a href="#" onclick="$(this).closest(\'.alert\').fadeOut(400);" class="close">\u00d7</a>'+a+"</div>")}
var tooltipManager={afficher:function(a){$("body .conteneurTips").remove();var b=$("<span/>").attr("class","conteneurTips").html($(this).attr("title"));$(this).attr("tips",$(this).attr("title")).attr("title","");$("body").append(b);b.fadeIn().css("top",a.pageY).css("left",a.pageX+10);return!1},cacher:function(a){$(this).attr("title",$(this).attr("tips")).attr("tips","");$("body .conteneurTips").fadeOut()},init:function(){$(".tips").hover(this.afficher,this.cacher)}};$(document).ready(function(){tooltipManager.init()});
$("#contenuEA").ready(function(){$("#contenuEA").on({mouseenter:function(){$(".btCheckbox",this).stop().animate({left:$(".btCheckbox",this).hasClass("selected")?0:47},180)},mouseleave:function(){$(".btCheckbox",this).stop().animate({left:$(".btCheckbox",this).hasClass("selected")?47:0},180)},click:function(){$(".btCheckbox",this).toggleClass("selected");$(this).trigger($(".btCheckbox",this).hasClass("selected")?"selected":"unselected")}},".checkboxCustom");$("#contenuEA").on("click",".checkboxCustomLabel",
function(){$(this).siblings(".checkboxCustom").trigger("mouseenter").trigger("click")});$("#contenuEA").on("focus",".formError",function(){$(this).removeClass("formError")})});$(document).ready(function(){$(".creditCard-off").on("click",function(){$("input[type=hidden][name=type_carte]").val($(this).attr("rel"))})});function num_format(a){aTemp=(a+"").split(".");if(!aTemp[1])return aTemp[0];for(;2>aTemp[1].length;)aTemp[1]+="0";return aTemp[0]+"."+aTemp[1]}
$(function(){$("button[click-once], input[type=submit][click-once]").click(function(){$(this).css("visibility","hidden")})});