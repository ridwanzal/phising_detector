$(function(){$("form.jsFormFocus").each(function(){if(!$(":text, :password",this).filter("[value='']:visible").filter(":first").focus().length){$(":text, :password",this).filter(":visible").each(function(){var A=$(this);if(A.val()===A.prev().attr("placeholder")){return !A.focus()}})}})});(function(A){A.hashNavigation=function(D,E,B){if(!location.hash){return }var C=location.hash.replace("%23","#").split("#")[D];C?E(C):B&&B()}}(jQuery));