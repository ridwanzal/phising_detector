var pix=pix||{};pix.ad_depend_id=[];pix.ad_depend_count=[];pix.ad_depend_group=[];pix.ad_extra_data=[];pix.ad_field_embed=[];pix.ad_embed_field=[];pix.ad_autoplay=[];pix.ad_noautoplay=[];pix.ad_loaded=function(field,embedid,dependid,autoplay){pix.ad_field_embed[""+field]=embedid;pix.ad_embed_field[""+embedid]=field;pix.ad_autoplay[""+embedid]=autoplay;if(dependid&&embedid!="PT"+dependid){pix.ad_depend_count["PT"+dependid]++}if(dependid){if(!pix.ad_depend_group["PT"+dependid]){pix.ad_depend_group["PT"+dependid]=[]}pix.ad_depend_id[embedid]="PT"+dependid;pix.ad_depend_group["PT"+dependid].push(embedid)}var swf_interval=setInterval(function(){var target=jQuery("#"+embedid),dependid,field=pix.ad_embed_field[embedid];if(target.length>0&&"undefined"!==typeof target.get(0).PercentLoaded&&target.get(0).PercentLoaded()>=100){if(!!(dependid=pix.ad_depend_id[embedid])){pix.ad_depend_count[dependid]--;if(pix.ad_depend_count[dependid]>0){return}field=pix.ad_embed_field[dependid]}jQuery("#"+embedid).addClass("swf_ready");pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)");clearInterval(swf_interval);jQuery(window).scroll()}},50)};pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)");jQuery(function(){var resize_or_scroll=function(){if(!pix.ad_embeds.length){return}pix.ad_embeds.each(function(index){var embed=pix.ad_embeds.eq(index),embedid=embed.attr("id"),dependid,field,target=jQuery("#"+embedid);if(embed.offset()&&embed.offset().top+embed.height()>jQuery(window).scrollTop()&&embed.offset().top<jQuery(window).scrollTop()+jQuery(window).height()){field=pix.ad_embed_field[embedid];if(!!(dependid=pix.ad_depend_id[embedid])){pix.ad_depend_count[dependid]--;if(pix.ad_depend_count[dependid]>0){return}field=pix.ad_embed_field[dependid]}if(pix.ad_load_init[field]){pix.ad_load_init[field](embedid)}if(pix.ad_autoplay[embedid]){if(!pix.get_cookie(embedid+"_played")&&!pix.ad_noautoplay[field]){pix.ad_load(field);pix.set_cookie(embedid+"_played",true)}else if(target.length>0){target.get(0).TGotoLabel("/","close")}}embed.addClass("swf_loaded");pix.ad_embeds=jQuery(".swf_ready:not(.swf_loaded)")}})};jQuery(window).scroll(resize_or_scroll);jQuery(window).resize(resize_or_scroll)});pix.get_cookie=function(key){var all_cookies=document.cookie.split(";"),i,cookie;for(i=0;i<all_cookies.length;i++){cookie=all_cookies[i].split("=");if(cookie[0].replace(/^\s+|\s+$/g,"")==key){return cookie[1]}}return};pix.set_cookie=function(key,value){var d=new Date,month,expires_date;if(null===value){month=d.getMonth()-1;expires_date=new Date(d.getFullYear()+"/"+month+"/"+d.getDate()+" 23:59:59");document.cookie=key+"="+value+";domain="+document.location.hostname+";expires="+expires_date.toGMTString()}else{month=d.getMonth()+1;expires_date=new Date(d.getFullYear()+"/"+month+"/"+d.getDate()+" 23:59:59");document.cookie=key+"="+value+";domain="+document.location.hostname+";expires="+expires_date.toGMTString()}};pix.ad_load_func=[];pix.ad_load_init=[];pix.ad_retry=[];pix.ad_load=function(fieldid){var embedid=pix.ad_field_embed[fieldid],parentembedid,childembedid,i;if(!embedid){return}function onerror(){pix.ad_load(fieldid)}parentembedid=pix.ad_depend_id[embedid];if(parentembedid){for(i in pix.ad_depend_group[parentembedid]){childembedid=pix.ad_depend_group[parentembedid][i];try{jQuery("#"+childembedid)[0].TGotoLabel("/","open")}catch(e){pix.ad_retry[fieldid]=pix.ad_retry[fieldid]?pix.ad_retry[fieldid]+1:1;if(pix.ad_retry[fieldid]<10){setTimeout(onerror,50);return}}}}else{try{jQuery("#"+embedid)[0].TGotoLabel("/","open")}catch(error){pix.ad_retry[fieldid]=pix.ad_retry[fieldid]?pix.ad_retry[fieldid]+1:1;if(pix.ad_retry[fieldid]<10){setTimeout(function(){pix.ad_load(fieldid)},50);return}}}if(pix.ad_load_func[fieldid]){pix.ad_load_func[fieldid](embedid)}};pix.ad_unload_func=[];pix.ad_unload=function(fieldid){var embedid=pix.ad_field_embed[fieldid],i;if(pix.ad_unload_func[fieldid]){pix.ad_unload_func[fieldid](embedid)}for(i in pix.ad_depend_group[embedid]){jQuery("#"+pix.ad_depend_group[embedid][i])[0].TGotoLabel("/","close")}};