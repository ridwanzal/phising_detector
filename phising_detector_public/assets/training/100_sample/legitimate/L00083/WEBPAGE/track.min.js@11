window.Track=function(a,c,b){if(!(""+document.location.hostname).match(/(^|\.)xhamster\.com$/g)&&!b)return!1;b=!0===b?0:Math.ceil(Math.random()/.5);var d=Math.round(1E6*Math.random()),e=encodeURIComponent(document.referrer||"");a=encodeURIComponent(""+a);a=[0===b?"/cnt.php":"//cnt"+b+".xhamster.com/cnt.php","?rf=",e,"&srv=",a,!0===c?"":"&"+c,"&_=",d].join("");(new Image).src=a};